  pod 'ReactorKit'

  target 'TyperMemoTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
  end

  target 'TyperMemoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
